<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/backgroundColor">

    <LinearLayout
        android:id="@+id/first_innings_button_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp">

        <Button
            android:id="@+id/first_innings_calc_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/button_design"
            android:clickable="true"
            android:text="@string/calculate"
            android:textColor="@color/white"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/first_innings_final_total_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@id/first_innings_button_container"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="vertical"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingEnd="10dp"
            android:paddingStart="15dp"
            android:weightSum="8">

            <TextView
                android:id="@+id/first_innings_team1_final_total_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/team_1_final_total"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="@color/black"/>

            <EditText
                android:id="@+id/first_innings_team1_final_total_et"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:digits="0123456789"
                android:gravity="center"
                android:imeOptions="actionNext"
                android:inputType="number"
                android:maxLength="4"
                android:nextFocusForward="@+id/first_innings_team2_overs_et"
                android:textAppearance="?android:attr/textAppearanceSmall"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingEnd="10dp"
            android:paddingStart="15dp"
            android:weightSum="8">

            <TextView
                android:id="@+id/first_innings_team2_overs_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/team2overs_first_innings"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="@color/black"/>

            <EditText
                android:id="@+id/first_innings_team2_overs_et"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:digits="0123456789."
                android:gravity="center"
                android:imeOptions="actionDone"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textAppearance="?android:attr/textAppearanceSmall"/>
        </LinearLayout>
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/first_innings_final_total_container"
        android:fillViewport="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            tools:context=".Scenario2">

            <LinearLayout
                android:id="@+id/G50_spinner_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/g50_expected_average_score"/>

                <Spinner
                    android:id="@+id/g50_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/total_overs_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/G50_spinner_container"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:weightSum="8">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:orientation="vertical"
                    android:paddingEnd="0dp"
                    android:paddingStart="10dp">

                    <TextView
                        android:id="@+id/number_overs_text_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp"
                        android:text="@string/start_inning_1_overs"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="@color/black"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="6"
                    android:orientation="vertical"
                    android:paddingEnd="10dp"
                    android:paddingStart="0dp">

                    <EditText
                        android:id="@+id/first_innings_number_overs_edit_text"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="0123456789."
                        android:gravity="center"
                        android:hint="@string/overs"
                        android:imeOptions="actionNext"
                        android:nextFocusForward="@+id/first_innings_which_over_interruption_1_et"
                        android:inputType="numberDecimal"
                        android:maxLength="4"
                        android:paddingTop="15dp"
                        android:textAppearance="?android:attr/textAppearanceSmall"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/first_innings_interruption_spinner_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/total_overs_container"
                android:layout_marginTop="20dp"
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/number_of_interruptions"/>

                <Spinner
                    android:id="@+id/first_innings_interruptions_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

            </LinearLayout>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view_1_fi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/first_innings_interruption_spinner_container"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                card_view:cardUseCompatPadding="true"
                card_view:cardElevation="4dp"
                card_view:cardBackgroundColor="@color/white"
                card_view:cardCornerRadius="10dp">

                <LinearLayout
                    android:id="@+id/first_innings_interruption_1_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/first_innings_interruption_spinner_container"
                    android:orientation="vertical"
                    >

                    <TextView
                        android:id="@+id/first_innings_interruption_1_tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:text="@string/interruption_1"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="@color/black"
                        android:textStyle="bold|italic"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:paddingEnd="10dp"
                        android:paddingStart="20dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_which_over_interruption_1_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:text="@string/interruption_1_over"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@id/first_innings_which_over_interruption_1_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789."
                                android:gravity="center"
                                android:hint="@string/overs"
                                android:inputType="numberDecimal"
                                android:maxLength="4"
                                android:nextFocusForward="@+id/first_innings_total_interruption_1_et"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_total_interruption_1_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:text="@string/total_at_interruption"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@id/first_innings_total_interruption_1_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789"
                                android:gravity="center"
                                android:hint="@string/total"
                                android:inputType="number"
                                android:maxLength="4"
                                android:nextFocusForward="@+id/first_innings_wickets_lost_interruption_1_et"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>
                        </LinearLayout>

                        <LinearLayout
