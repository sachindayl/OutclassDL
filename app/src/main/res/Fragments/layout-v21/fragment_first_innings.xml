                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_wickets_lost_interruption_1_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:text="@string/interruption_1_wickets"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@+id/first_innings_wickets_lost_interruption_1_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789"
                                android:gravity="center"
                                android:hint="@string/wickets"
                                android:inputType="number"
                                android:maxLength="2"
                                android:nextFocusForward="@+id/first_innings_overs_remaining_interruption_1_et"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_overs_remaining_interruption_1_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:text="@string/interruption_1_remaining"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@id/first_innings_overs_remaining_interruption_1_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789."
                                android:gravity="center"
                                android:hint="@string/overs"
                                android:inputType="numberDecimal"
                                android:maxLength="4"
                                android:nextFocusForward="@+id/first_innings_which_over_interruption_2_et"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view_2_fi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/card_view_1_fi"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                card_view:cardUseCompatPadding="true"
                card_view:cardElevation="4dp"
                card_view:cardBackgroundColor="@color/white"
                card_view:cardCornerRadius="10dp">

                <LinearLayout
                    android:id="@+id/first_innings_interruption_2_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/first_innings_interruption_1_container"
                    android:orientation="vertical"
                    >

                    <TextView
                        android:id="@+id/first_innings_interruption_2_tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:text="@string/interruption_2"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="@color/black"
                        android:textStyle="bold|italic"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:paddingEnd="10dp"
                        android:paddingStart="20dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_which_over_interruption_2_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:text="@string/interruption_1_over"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@id/first_innings_which_over_interruption_2_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789."
                                android:gravity="center"
                                android:hint="@string/overs"
                                android:inputType="numberDecimal"
                                android:maxLength="4"
                                android:nextFocusForward="@+id/first_innings_total_interruption_2_et"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_total_interruption_2_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:paddingBottom="10dp"
                                android:text="@string/total_at_interruption"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@+id/first_innings_total_interruption_2_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789"
                                android:gravity="center"
                                android:hint="@string/total"
                                android:inputType="number"
                                android:maxLength="4"
                                android:nextFocusForward="@+id/first_innings_wickets_lost_interruption_2_et"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_wickets_lost_interruption_2_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:paddingBottom="10dp"
                                android:text="@string/interruption_1_wickets"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@id/first_innings_wickets_lost_interruption_2_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789"
                                android:gravity="center"
                                android:hint="@string/wickets"
                                android:inputType="number"
                                android:maxLength="2"
                                android:nextFocusForward="@+id/first_innings_overs_remaining_interruption_2_et"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_overs_remaining_interruption_2_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:text="@string/interruption_1_remaining"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@id/first_innings_overs_remaining_interruption_2_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789."
                                android:gravity="center"
                                android:hint="@string/overs"
                                android:inputType="numberDecimal"
                                android:maxLength="4"
                                android:nextFocusForward="@+id/first_innings_which_over_interruption_3_et"
                                android:paddingBottom="15dp"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/card_view_3_fi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/card_view_2_fi"
                android:layout_gravity="center"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="5dp"
                card_view:cardUseCompatPadding="true"
                card_view:cardElevation="4dp"
                card_view:cardBackgroundColor="@color/white"
                card_view:cardCornerRadius="10dp">

                <LinearLayout
                    android:id="@+id/first_innings_interruption_3_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/first_innings_interruption_2_container"
                    android:orientation="vertical"
                    >

                    <TextView
                        android:id="@+id/first_innings_interruption_3_tv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:text="@string/interruption_3"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="@color/black"
                        android:textStyle="bold|italic"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:paddingEnd="10dp"
                        android:paddingStart="20dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_which_over_interruption_3_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:text="@string/interruption_1_over"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@id/first_innings_which_over_interruption_3_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789."
                                android:gravity="center"
                                android:hint="@string/overs"
                                android:inputType="numberDecimal"
                                android:maxLength="4"
                                android:nextFocusForward="@+id/first_innings_total_interruption_3_et"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_total_interruption_3_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:paddingBottom="10dp"
                                android:text="@string/total_at_interruption"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@id/first_innings_total_interruption_3_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789"
                                android:gravity="center"
                                android:hint="@string/total"
                                android:inputType="number"
                                android:maxLength="4"
                                android:nextFocusForward="@+id/first_innings_wickets_lost_interruption_3_et"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_wickets_lost_interruption_3_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:paddingBottom="10dp"
                                android:text="@string/interruption_1_wickets"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@id/first_innings_wickets_lost_interruption_3_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789"
                                android:gravity="center"
                                android:hint="@string/wickets"
                                android:inputType="number"
                                android:maxLength="2"
                                android:nextFocusForward="@+id/first_innings_overs_remaining_interruption_3_et"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="8">

                            <TextView
                                android:id="@+id/first_innings_overs_remaining_interruption_3_tv"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="2"
                                android:text="@string/interruption_1_remaining"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:textColor="@color/black"/>

                            <EditText
                                android:id="@id/first_innings_overs_remaining_interruption_3_et"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="6"
                                android:digits="0123456789."
                                android:gravity="center"
                                android:hint="@string/overs"
                                android:inputType="numberDecimal"
                                android:maxLength="4"
                                android:paddingBottom="15dp"
                                android:textAppearance="?android:attr/textAppearanceSmall"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>

